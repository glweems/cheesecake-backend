{
  "name": "cheat-day-cheesecake-backend",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "main": "src/server.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc -p ./",
    "start": "node ./dist/server.js",
    "serve": "npm run build && npm run start",
    "watch": "tsnd --respawn ./src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "watch-ts": "tsc -w -p ./",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "reset": "npm run migrate && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glweems/typeScript-express-starter.git"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "author": "Garrett Weems <gwgraphicdesign@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/typeScript-express-starter/issues"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.15.4",
    "express-promise-router": "^3.0.3",
    "knex": "^0.19.0",
    "objection": "^1.6.9",
    "pg": "^7.11.0",
    "ts-node": "^8.3.0",
    "validatorjs": "^3.13.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.0.37",
    "@types/knex": "0.16.1",
    "@types/lusca": "^1.6.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "^14.0.12",
    "@types/validatorjs": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.12.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  }
}
